<h1>Cartões</h1>

<a href="/usuarios/{{usuario.id}}/novoCartao">Cadastrar Cartão</a>
<table border="1">
  <tr>
    <th>Número</th>
    <th>Nome</th>
  </tr>
  {{#each cartoes}}
    <tr>
      <td>{{this.numero}}</td>
      <td>{{this.nome}}</td>
    </tr>
    
  {{/each}}
  <button><a href="http://localhost:8000/usuarios">Voltar</a></button>
</table>